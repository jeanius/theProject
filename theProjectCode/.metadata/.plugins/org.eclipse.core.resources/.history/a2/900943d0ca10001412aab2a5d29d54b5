package com.jeanpower.reggieproject;


import android.util.Log;

public class PauseInstruction implements Runnable{
	
	
	@Override
	public void run() {
		
		Log.d("I am in the doinbackground", "yes");
		
		try {
		Thread.sleep(1000);

		}
		catch (Exception e){
			Log.d("This was interrupted", "interrupted");
		}		
		
		
	}
}
